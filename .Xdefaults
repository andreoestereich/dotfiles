! xft fonts
!------------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.lcdfilter:  lcddefault
Xft.hinting:    true
Xft.hintstyle:  hintfull

! xterm
!----------------------------------------------------------------------

XTerm*visualbell: true

!xterm*faceName: xft:Inconsolata-dz for powerline:pixelsize=14:antialias=true
XTerm*utf8: 1
XTerm.vt100.faceName: Hack:size=12:antialias=true
XTerm.vt100.font: 7x13
XTerm*background:            #1c1c1c
XTerm*foreground:            #626262
XTerm*fading:                40
XTerm*fadeColor:             #1c1c1c
XTerm*cursorColor:           #8a8a8a
XTerm*pointerColorBackground:#585858
XTerm*pointerColorForeground:#8a8a8a


!! black dark/light
XTerm*color0:                #262626
XTerm*color8:                #1c1c1c

!! red dark/light
XTerm*color1:                #d70000
XTerm*color9:                #d75f00

!! green dark/light
XTerm*color2:                #5f8700
XTerm*color10:               #585858

!! yellow dark/light
XTerm*color3:                #af8700
XTerm*color11:               #626262

!! blue dark/light
XTerm*color4:                #0087ff
XTerm*color12:               #808080

!! magenta dark/light
XTerm*color5:                #af005f
XTerm*color13:               #5f5faf

!! cyan dark/light
XTerm*color6:                #00afaf
XTerm*color14:               #8a8a8a

!! white dark/light
XTerm*color7:                #e4e4e4
XTerm*color15:               #ffffd7


!! Transparency (0-255):
*.alpha: 200

! urxvt
!---------------------------------------------------------------------

URxvt*font:Hack:size=12
URxvt.matcher.button:   1
URxvt.letterSpace: -1

URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/qutebrowser
URxvt.url-select.underline: true

URxvt.perl-ext-common: keyboard-select
URxvt.keysym.M-c: perl:keyboard-select:activate

URxvt.keysym.M-s: perl:keyboard-select:search

URxvt*scrollstyle:plain
URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*transpscrollbar: true
URxvt*scrollBar_floating: true
URxvt*saveLines: 1000000
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

urxvt*transparent: true
urxvt*shading: 10

